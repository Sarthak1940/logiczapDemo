{
    "rewrites": [
      { "source": "/(.*)", "destination": "/" }
    ],
    "redirects": [
      { "source": "/old-route", "destination": "/new-route", "permanent": true }
    ],
    "headers": [
      { "source": "/(.*)", "headers": [{ "key": "Cache-Control", "value": "public, max-age=3600" }] }
    ]
  }
  